{
    "executionRoleArn": "arn:aws:iam::ACCT_ID:role/ecs-sample-role",
    "containerDefinitions": [
        {
            "name": "api-test",
            "image": "ECR_URI:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "containerPort": 8000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "api-test"
}